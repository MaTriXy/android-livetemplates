<templateSet group="Appcom Dagger 2">
  <template name="ppf" value="@Provides @PerFragment public $CLASS$ provide$CLASS$($CLASS$Impl $CLASS_COPY$Impl){&#10;  return $CLASS_COPY$Impl;&#10;} " description="Provides class with inject constructor &quot;per Fragment&quot;" toReformat="true" toShortenFQNames="true">
    <variable name="CLASS" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="CLASS_COPY" expression="decapitalize(CLASS)" defaultValue="" alwaysStopAt="false" />
    <context>
      <option name="JAVA_CODE" value="true" />
    </context>
  </template>
  <template name="inj" value="@Inject $END$" description="@Inject - Annotation" toReformat="true" toShortenFQNames="true">
    <context>
      <option name="JAVA_DECLARATION" value="true" />
    </context>
  </template>
  <template name="pro" value="@Provides $END$" description="@Provides - Annotation" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="JAVA_DECLARATION" value="true" />
    </context>
  </template>
  <template name="sing" value="@Singleton $END$" description="@Singleton - Annotation" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="JAVA_DECLARATION" value="true" />
    </context>
  </template>
  <template name="ppa" value="@Provides @PerActivity public $CLASS$ provide$CLASS$($CLASS$Impl $CLASS_COPY$Impl){&#10;  return $CLASS_COPY$Impl;&#10;} " description="Provides class with inject constructor &quot;per Activity&quot;" toReformat="true" toShortenFQNames="true">
    <variable name="CLASS" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="CLASS_COPY" expression="decapitalize(CLASS)" defaultValue="" alwaysStopAt="false" />
    <context>
      <option name="JAVA_CODE" value="true" />
    </context>
  </template>
  <template name="pps" value="@Provides @Singleton public $CLASS$ provide$CLASS$($CLASS$Impl $CLASS_COPY$Impl){&#10;  return $CLASS_COPY$Impl;&#10;} " description="Provides class with inject constructor &quot;as Singleton&quot;" toReformat="true" toShortenFQNames="true">
    <variable name="CLASS" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="CLASS_COPY" expression="decapitalize(CLASS)" defaultValue="" alwaysStopAt="false" />
    <context>
      <option name="JAVA_CODE" value="true" />
    </context>
  </template>
  <template name="injc" value="@Inject public $CLASS$() {  &#10;}" description="Constructor with @Inject - Annotation" toReformat="true" toShortenFQNames="true">
    <variable name="CLASS" expression="className()" defaultValue="" alwaysStopAt="false" />
    <context>
      <option name="JAVA_DECLARATION" value="true" />
    </context>
  </template>
</templateSet>